   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "netflix_titles = \"netflix_titles.csv\"\n",
    "df = pd.read_csv(netflix_titles)\n",
    "def get_director(df):\n",
    "    USA_director = df[df['country']=='United States']\n",
    "    df['director'] = USA_director['director'].str.split(', ')\n",
    "    df = df.explode('director')\n",
    "    count_per_director = df.groupby('director')['show_id'].nunique()\n",
    "\n",
    "    count_per_director = count_per_director.sort_values(ascending= False)\n",
    "    top_director = count_per_director.head(20)\n",
    "    return(top_director)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
